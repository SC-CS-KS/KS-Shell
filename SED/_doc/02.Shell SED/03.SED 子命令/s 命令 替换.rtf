\par \plain \f1\fs20\cf4 \uc2\u19981 \'b2\'bb\u35770 \'c2\'db\u20160 \'ca\'b2\u20040 \'c3\'b4\u23383 \'d7\'d6\u31526 \'b7\'fb\u65292 \'a3\'ac\u32039 \'bd\'f4\u36319 \'b8\'fa\u30528 \'d7\'c5s\u21629 \'c3\'fc\u20196 \'c1\'ee\u30340 \'b5\'c4\u37117 \'b6\'bc\u34987 \'b1\'bb\u35748 \'c8\'cf\u20026 \'ce\'aa\u26159 \'ca\'c7\u26032 \'d0\'c2\u30340 \'b5\'c4\u20998 \'b7\'d6\u38548 \'b8\'f4\u31526 \'b7\'fb\u65292 \'a3\'ac\u25152 \'cb\'f9\u20197 \'d2\'d4\u65292 \'a3\'ac\ldblquote #\rdblquote \u22312 \'d4\'da\u36825 \'d5\'e2\u37324 \'c0\'ef\u26159 \'ca\'c7\u20998 \'b7\'d6\u38548 \'b8\'f4\u31526 \'b7\'fb\u65292 \'a3\'ac\u20195 \'b4\'fa\u26367 \'cc\'e6\u20102 \'c1\'cb\u40664 \'c4\'ac\u35748 \'c8\'cf\u30340 \'b5\'c4\ldblquote /\rdblquote \u20998 \'b7\'d6\u38548 \'b8\'f4\u31526 \'b7\'fb\u12290 \'a1\'a3\uc1\plain \f1\fs20 \uc2\u34920 \'b1\'ed\u31034 \'ca\'be\u25226 \'b0\'d1\u25152 \'cb\'f9\u26377 \'d3\'d010\u26367 \'cc\'e6\u25442 \'bb\'bb\u25104 \'b3\'c9100\u12290 \'a1\'a3 \uc1
\par \plain \f1\fs20\cf1 #sed  's#\\(chan\\)sunny#\\1guang#' chan.t  
