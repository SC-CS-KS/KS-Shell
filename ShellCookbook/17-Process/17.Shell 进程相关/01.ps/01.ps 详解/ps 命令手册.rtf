\par \~
\par \plain \f1\fs21\cf1 # \uc2\u27880 \'d7\'a2\u37322 \'ca\'cd \u65306 \'a3\'balstart \u34920 \'b1\'ed\u31034 \'ca\'be\u21629 \'c3\'fc\u20196 \'c1\'ee\u21551 \'c6\'f4\u21160 \'b6\'af\u30340 \'b5\'c4\u26102 \'ca\'b1\u38388 \'bc\'e4\uc1
